i : p1,p2
	~	
	~		|

j <-> i
	~	
	~		|

res type : dres,fres
	~	
	~		|

s : eu,sp
	~	
	~		|

********************************************************
	.Control
********************************************************~
		Simulation Control Parameters
	|

FINAL TIME  = 20
	~	Year
	~	The final time for the simulation.
	|

INITIAL TIME  = 0
	~	Year
	~	The initial time for the simulation.
	|

SAVEPER  = 
        TIME STEP
	~	Year [0,?]
	~	The frequency with which output is stored.
	|

TIME STEP  = 0.0625
	~	Year [0,?]
	~	The time step for the simulation.
	|

********************************************************
	.market
********************************************************~
	|

a  = 0.01
	~	1/Year [0,0.1,0.01]
	~	Joint advertising effectiveness for all platforms.
	|

Adopter Fraction[eu,i]  = 
        Adopters[eu,i] 
             / N ~~|
Adopter Fraction[sp,i]  = 
        IF THEN ELSE ( SWITCH OPEN 
                  > 0.5, 
             sum ( Adopters[sp,i!] ) 
                  / N , 
             Adopters[sp,i] 
                  / N )
	~	Dmnl
	~		|

Adopters[s,i]  = 
        INTEG( Adoption Rate[s,i] 
                  + Switching Rate[s,i] 
                  - Discard Rate[s,i] , 
             Initial Adopters )
	~	User
	~	Corresponds to equation 2.
	|

Adoption Fraction[s,i]  = 
        zidz ( Value to Non Adopters[s,i] , 
             sum ( Value to Non Adopters[s,i!] 
                       + V ref ) )
	~	Dmnl
	~	Corresponds to equation 6.
	|

Adoption from Advertising[s,i]  = 
        IF THEN ELSE ( Time 
                  < T 
                  :AND: Time 
                       >= T0[i] , 
             a 
                  * Potential Adopters[s] 
                  / ELMCOUNT(i), 
             0)
	~	User/Year
	~		|

Adoption from Word of Mouth[s,i]  = 
        zidz ( c 
                  * Adoption Fraction[s,i] 
                  * Potential Adopters[s] 
                  * Adopters[s,i] , 
             N )
	~	User/Year
	~		|

Adoption Rate[s,i]  = 
        min ( Potential Adopters[s] 
                  / TIME STEP , 
             ( Adoption from Advertising[s,i] 
                  + Adoption from Word of Mouth[s,i] ) )
	~	User/Year
	~	Corresponds to equation 3.
	|

c  = 5
	~	1/Year [1,20,1]
	~		|

Combined adopter fraction[s]  = 
        sum ( Adopters[s,i!] ) 
             / N
	~	Dmnl
	~		~	:SUPPLEMENTARY 
	|

Discard Fraction[s,i]  = 
        zidz ( V ref , 
             ( sum ( Value to Adopters[s,i,j!] ) 
                  + V ref ) )
	~	Dmnl
	~	Corresponds to equation 7.
	|

Discard Rate[s,i]  = 
        SWITCH DISCARDS 
             * Adopters[s,i] 
             * Discard Fraction[s,i] 
             / tau
	~	User/Year
	~	Corresponds to equation 4.
	|

Indicated Switchers[s,i,j]  = 
        ( Switch Fraction[s,i,j] 
             * Adopters[s,i] ) 
             / tau
	~	User/Year
	~	The switcher outflow from i to j before taking into account the total \
		possible outflow. Used to calculate the switching rate.
	|

Initial Adopters  = 0
	~	User [0,1,0.1]
	~		|

N  = 1000
	~	User
	~		|

Potential Adopters[s]  = 
        INTEG( sum ( Discard Rate[s,i!] ) 
                  - sum ( Adoption Rate[s,i!] ) , 
             N 
                  - sum ( Adopters[s,i!] ) )
	~	User
	~	Corresponds to equation 1.
	|

Switch Fraction[s,i,j]  = 
        IF THEN ELSE ( i
                  = j, 
             0, 
             zidz ( Value to Adopters[s,i,j] , 
                  sum ( Value to Adopters[s,i,j!] 
                            + V ref ) ) )
	~	Dmnl
	~	Switching from platform i to j. Corresponds to equation 8.
	|

Switcher Net Inflow[s,i,j]  = 
        Switchers[s,j,i] 
             - Switchers[s,i,j]
	~	User/Year
	~	The net inflow of switchers from platform j to i. Used to calculate the \
		switching rate.
	|

Switchers[s,i,j]  = 
        zidz ( Indicated Switchers[s,i,j] , 
             sum ( Indicated Switchers[s,i,j!] ) ) 
             * min ( Adopters[s,i] 
                       / TIME STEP 
                       - Discard Rate[s,i] , 
                  sum ( Indicated Switchers[s,i,j!] ) )
	~	User/Year
	~	The switcher outflow taking into account the total possible outflow. Used \
		to calculate the switching rate.
	|

Switching Rate[eu,i]  = 
        sum ( Switcher Net Inflow[eu,i,j!] ) ~~|
Switching Rate[sp,i]  = 
        sum ( Switcher Net Inflow[sp,i,j!] ) 
             * SWITCH SP SWITCHERS
	~	User/Year
	~	Corresponds to equation 5.
	|

T  = 4
	~	Year [0,10,0.1]
	~		|

T0[p1]  = 0 ~~|
T0[p2]  = 0.125
	~	Year
	~		|

tau  = 1
	~	Year [0.5,10,0.25]
	~		|

V ref  = 3
	~	Dmnl [0,10,0.1]
	~		|

********************************************************
	.resource
********************************************************~
	|

M ref  = 0.2
	~	Dmnl [0,1,0.01]
	~		|

MS Gap[eu,i]  = 
        SMOOTH ( Adopter Fraction[eu,i] 
                  - M ref , 
             tau' )
	~	Dmnl
	~	Corresponds to equation 12.
	|

tau'  = 1
	~	Year [0.25,2,0.25]
	~		|

Cumulative Adopters[eu,i]  = 
        INTEG( Adoption Rate[eu,i] 
                  + max ( 0, 
                       Switching Rate[eu,i] ) , 
             Initial Adopters )
	~	User
	~	Used for calculating the data per user. Corresponds to equation 11.
	|

beta  = 4
	~	1/Year [0,100,1]
	~		|

Competitive Effort[i]  = 
        IF THEN ELSE ( SWITCH FULL CE , 
             1, 
             max ( 0, 
                  min ( 1, 
                       1
                            - MS Gap[eu,i] 
                                 * beta ) ) )
	~	1/Year
	~	Corresponds to equation 13.
	|

Data per User[i]  = 
        zidz ( Resources[i,dres] , 
             Cumulative Adopters[eu,i] )
	~	Resource Unit/User
	~	Corresponds to equation 14.
	|

Data Rate from Switchers[i]  = 
        theta 
             * sum ( max ( 0, 
                       Switcher Net Inflow[eu,i,j!] ) 
                       * Data per User[j!] )
	~	Resource Unit/Year
	~	Corresponds to equation 15.
	|

Expenses[i]  = 
        Competitive Effort[i] 
             * Resources[i,fres]
	~	Resource Unit/Year
	~		|

F0  = 1000
	~	Resource Unit [0,2000,10]
	~	Total initial financial resources among rival platforms.
	|

ra  = 1
	~	Resource Unit/(Year*User) [0.1,1000,1]
	~		|

re  = 0
	~	1/Year [0,1,0.01]
	~		|

Resource Accumulation[i]  = 
        Adopters[eu,i] 
             * ra
	~	Resource Unit/Year
	~		|

Resource Depletion[i,dres]  = 
        Resources[i,dres] 
             * re ~~|
Resource Depletion[i,fres]  = 
        Resources[i,fres] 
             * IF THEN ELSE ( MS Gap[eu,i] 
                       > 0, 
                  re , 
                  0)
	~	Resource Unit/Year
	~		|

Resources[i,fres]  = 
        INTEG( max ( - Resources[i,fres] 
                       / TIME STEP , 
                  Resource Accumulation[i] 
                       - Resource Depletion[i,fres] 
                       - Expenses[i] ) , 
             F0 
                  / ELMCOUNT(i)) ~~|
Resources[i,dres]  = 
        INTEG( Resource Accumulation[i] 
                  + Data Rate from Switchers[i] 
                  - Resource Depletion[i,dres] , 
             0)
	~	Resource Unit
	~	Corresponds to equations 9 and 10 (fres: financial resources, dres: data \
		resources).
	|

theta  = 0
	~	Dmnl [0,1,0.1]
	~		|

********************************************************
	.switch
********************************************************~
	|

SWITCH DISCARDS  = 1
	~	Dmnl [0,1,1]
	~		|

SWITCH FULL CE  = 0
	~	Dmnl [0,1,1]
	~		|

SWITCH OPEN  = 0
	~	Dmnl [0,1,1]
	~		|

SWITCH SP SWITCHERS  = 1
	~	Dmnl [0,1,1]
	~		|

SWITCH USER DATA VALUE  = 1
	~	Dmnl [0,1,1]
	~		|

********************************************************
	.value
********************************************************~
	|

af ref  = 0.5
	~	Dmnl [0,1,0.1]
	~		|

alpha  = 0.7
	~	Dmnl [0,3,0.1]
	~		|

Data REF  = 
        max ( 1, 
             T ref 
                  * N 
                  * ra )
	~	Resource Unit
	~		|

gamma  = 0.7
	~	Dmnl [0,2,0.1]
	~		|

gamma'  = 1
	~	Dmnl [0,4,0.1]
	~		|

p ref  = 1
	~	1/Resource Unit
	~		|

Q0  = 1
	~	Dmnl
	~		|

qe  = 0
	~	1/Year [0,0.1,0.01]
	~		|

Quality[i]  = 
        INTEG( Expenses[i] 
                  * p ref 
                  * ( Resources[i,dres] 
                       / Data REF ) 
                       ^ gamma 
                  - Quality[i] 
                       * qe , 
             Q0 )
	~	Dmnl
	~	Corresponds to equation 16.
	|

Ref Data per User  = 
        ra 
             * T ref
	~	Resource Unit/User
	~		|

T ref  = 0.25
	~	Year [0.01,2,0.01]
	~		|

Value to Adopters[eu,i,j]  = 
        IF THEN ELSE ( SWITCH USER DATA VALUE 
                  > 0.5, 
             Value to Non Adopters[eu,j] 
                  * IF THEN ELSE ( i
                            = j, 
                       1
                            + ( Data per User[i] 
                                 / Ref Data per User ) 
                                 ^ gamma' , 
                       1
                            + ( theta 
                                 * Data per User[i] 
                                 / Ref Data per User ) 
                                 ^ gamma' ) , 
             Value to Non Adopters[eu,j] ) ~~|
Value to Adopters[sp,i,j]=
	Value to Non Adopters[sp,j]
	~	Dmnl
	~	The value of platform j to adopters of platform i. Corresponds to equation \
		18.
	|

Value to Non Adopters[eu,i]  = 
        Quality[i] 
             * ( Adopter Fraction[sp,i] 
                  / af ref ) 
                  ^ alpha ~~|
Value to Non Adopters[sp,i]  = 
        Quality[i] 
             * ( Adopter Fraction[eu,i] 
                  / af ref ) 
                  ^ alpha
	~	Dmnl
	~	Corresponds to equation 17.
	|

